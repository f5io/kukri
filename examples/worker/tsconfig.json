{
	"compilerOptions": {
		"target": "es2022",
		"lib": ["esnext"],
		"jsx": "react-jsx",
		"jsxImportSource": "kukri",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"types": ["@cloudflare/workers-types"],
		"resolveJsonModule": true,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true
	}
}
